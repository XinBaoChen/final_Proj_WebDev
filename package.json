{
  "name": "starter-server",
  "version": "1.0.0",
  "description": "backend for crud app",
  "main": "web-dev-server/app.js",
  "scripts": {
    "start": "node web-dev-server/app.js",
    "dev": "nodemon web-dev-server/app.js",
    "seed": "node web-dev-server/seed.js",
    "migrate": "node web-dev-server/database/migrations/runMigrations.js",
    "seed:ensure": "node web-dev-server/database/utils/seedIfEmpty.js",
    "checkdb": "node scripts/checkDb.js",
    "vercel-build": "npm --prefix web-dev-client install && npm --prefix web-dev-client run build && node scripts/copyBuild.js",
    "postinstall": "npm --prefix api install"
  },
  "author": "Your Name Here",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "pg": "^8.5.1",
    "pgtools": "^1.0.1",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
